}

phase2 //air
{
    transportModel  Newtonian;
    B               B [1 -1 -2 0 0 0 0] 0;
    rho0            rho0 [1 -3 0 0 0 0 0] 0.12;
    rho             rho [1 -3 0 0 0 0 0] 1;
    p0              p0 [1 -1 -2 0 0 0 0] 10320;
    gamma           gamma [0 0 0 0 0 0 0] 1.33;
    mu              mu [1 -1 -1 0 0 0 0] 1.3e-5;
    nu              nu [0 2 -1 0 0 0 0] 1.3e-5;
}

pMin           pMin [1 -1 -2 0 0 0 0] -1e11;
